              <%begin%>
                <%= image_tag (@tritonmun[1].image_url)%>
              <%rescue%>
                <img src="assets/image_2.jpg" alt="...">
              <%end%>
              <div class="caption">
                <h4>Caption Text</h4>
              </div>
            </div>
            <div class="side_img_2">
              <%begin%>
                <%= image_tag (@tritonmun[2].image_url)%>
              <%rescue%>
                <img src="assets/image_3.jpg" alt="...">
              <%end%>
              <div class="caption_1">
                <h4>Caption Text</h4>
              </div>
            </div>
          </div>
        </div>
        <div class="item">
