<div class="row">
  <div id="carousel-example-generic" class="carousel slide vertical" data-ride="carousel">
    <!-- Indicators -->
    <div class="col-sm-2 col-sm">
      <div class="carousel-indicators carousel_ind">
        <div data-target="#carousel-example-generic" data-slide-to="0" class="active thumb"><img src="http://placehold.it/100/e8117f/fff&amp;text=Product+Main"></div>
        <div data-target="#carousel-example-generic" data-slide-to="1" class="thumb"><img src="http://placehold.it/100/e8117f/fff&amp;text=Product+Main"></div>
        <div data-target="#carousel-example-generic" data-slide-to="2" class="thumb"><img src="http://placehold.it/100/e8117f/fff&amp;text=Product+Main"></div>
      </div>
    </div>
 
    <!-- Wrapper for slides -->
    <div class="col-sm-9" style="float:left; width: 84%; padding-top:50px">
      <div class="carousel-inner">
        <div class="item active">
          <div class="main_image">
            <%begin%>
              <%= image_tag (@tritonmun[0].image_url)%>
            <%rescue%>
              <img src="assets/image_1.jpg" alt="...">
            <%end%>
          </div>
          <div class="second_img">
