<div class="conference">
  
  <div class = "row header-title-bg" style = "margin: 2% 0% 3% 0%;">
    <div class="col-lg-12 text-center about-header-img">
      <h2 class = "section-heading">History</h2>
      <h3 class = "section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
    </div>
  </div>
  <div class="row">
  	<div class="col-md-10" style = "margin-left: 4%">
	  <ul class="cbp_tmtimeline">
		<% @histories.each do |history| %>
		  <li>
			<time class="cbp_tmtime" datetime="2013-04-10 18:30">
			  <span><%= history.year %></span>
			</time>
			<div class="cbp_tmicon">
		  	  <i class="icon-lightbulb"></i>
			</div>
			<div class="cbp_tmlabel">
			  <%= history.description.html_safe %>
			</div>
		  </li>
		<%end%>
	  </ul>
    </div>
  </div>
</div>
