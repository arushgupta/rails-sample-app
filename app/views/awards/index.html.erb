<div class="container" id="pricing-section">
  
  <div class="row" style = "margin-bottom: 3%; margin-top: 2%;">
    <div class="col-lg-12 text-center " style = "background-image: url('/assets/conference-title.jpg'); background-size: cover;">
      <h2 class = "section-heading">Awards</h2>
      <h3 class = "section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
    </div>
  </div> 
  
  <div class="main-low-margin col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-1 margin-top-100 ">
    <div id="pricing-table" class="row">
      <% @travelconferences.each do |tconference| %>
          <div class="col-md-4 col-sm-4">
            <ul class="plan-main">
              <li class="plan-name">
                <%= tconference.short_name %> <%= tconference.year %>
              </li>
              <li>
                <%= image_tag  tconference.travel_team_image_url.url %>
              </li>
            </ul>
              <% tconference.awards.each do |award|%>
              <ul class = "plain-main">
              <li>
                  <span class= "plan-price"><%= award.award_name %></span>
              </li>
            </ul>
          </div>
        <%end%>
      <%end%>
    </div>              
  </div>
</div>